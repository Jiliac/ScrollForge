# Prisma database (Supabase Postgres - use Transaction pooler URL)
DATABASE_URL=postgresql://postgres.[ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Supabase (public - safe for browser)
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...
NEXT_PUBLIC_SUPABASE_URL=https://xyz.supabase.co

# Supabase (secret - server-side only)
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Anthropic API key for Claude
ANTHROPIC_API_KEY=sk-ant-...

# OpenAI API key for GPT
OPENAI_API_KEY=

# Google Generative AI API key
GOOGLE_GENERATIVE_AI_API_KEY=

# Black Forest Labs API key for FLUX image generation
BFL_API_KEY=bfl_...

ELEVEN_LABS_API_KEY=

# Environment (development/production) - controls error detail exposure
NODE_ENV=development
